<!DOCTYPE html>
<html>
  <head>
    <title>JohariToo</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render user_nav %>
    <% flash.each do |key, value| %>
      <div class="alert alert-<%= key %>"><%= value %></div>
    <% end %>
    <div class='container'>
      <%= yield %>
    </div>
  </body>
</html>
